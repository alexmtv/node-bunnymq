  "bugs": {
    "url": "https://github.com/dial-once/node-rabbit-layer/issues"
  },
  "homepage": "https://github.com/dial-once/node-rabbit-layer#readme",
  "dependencies": {
    "amqplib": "0.4.0",
    "winston": "2.1.1"
  },
  "devDependencies": {
    "assert": "1.3.0",
    "node-uuid": "1.4.7"
  }
